{
  "name": "financial-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:security": "cd security-tests && playwright test --config=playwright.config.ts",
    "test:security:ui": "cd security-tests && playwright test --config=playwright.config.ts --ui",
    "test:performance": "cd performance-tests && playwright test",
    "test:load": "cd load-tests && playwright test",
    "type-check": "tsc --noEmit",
    "check-port": "node scripts/check-port.js",
    "dev:safe": "npm run check-port && npm run dev"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@sentry/nextjs": "^9.30.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@types/uuid": "^10.0.0",
    "@upstash/redis": "^1.35.0",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.522.0",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "recharts": "^2.15.4",
    "redis": "^5.5.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "playwright": "^1.53.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "typescript": "^5",
    "undici": "^7.10.0"
  }
}
